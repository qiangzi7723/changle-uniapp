.full-page {
  width: 100vw;
  height: 100vh;
}
.absolute-full-page {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
}
.profile {
  background-color: #f0e9e9;
  overflow: hidden;
  height: 100vh;
}
.profile .banner {
  width: 750rpx;
  height: 560rpx;
  background-size: contain;
  background-position: center;
  background-repeat: no-repeat;
  background-image: url(data:image/png;base64,);
  text-align: center;
  position: absolute;
  top: 0;
  z-index: 0;
}
.profile .banner .title {
  font-size: 36rpx;
  color: #fff;
  margin-top: 60rpx;
  display: block;
}
